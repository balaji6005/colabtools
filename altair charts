              "<script type=\"text/javascript\">\n",
              "  (function(spec, embedOpt){\n",
              "    let outputDiv = document.currentScript.previousElementSibling;\n",
              "    if (outputDiv.id !== \"altair-viz-64d538051145461b860194dcad15da7f\") {\n",
              "      outputDiv = document.getElementById(\"altair-viz-64d538051145461b860194dcad15da7f\");\n",
              "    }\n",
              "    const paths = {\n",
