      "source": [
        "import altair as alt\n",
        "import pandas as pd\n",
        "source=pd.DataFrame({'Activity':['sleeping','reading','eating','Tv','Excercise'],'Time':[8,10,2,1,3]})\n",
        "a=alt.Chart(source).transform_joinaggregate(t='sum(Time)').transform_calculate(PercentOfTime='datum.Time/datum.t').mark_bar().encode(alt.X('PercentOfTime:Q',axis=alt.Axis(format='.0%')),y='Activity')\n",
        "b=a.mark_text(align='left',baseline='middle',dx=3).encode(x='PercentOfTime:Q',y='Activity')\n",
        "(a+b)"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "execute_result",
          "data": {
            "text/plain": [
              "alt.LayerChart(...)"
            ],
