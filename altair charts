        "      BAR AND TICK CHART"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 364
        },
        "id": "oaW848YzWGa3",
        "outputId": "e893743a-f88f-4458-cb6a-7cca155ee42e"
      },
      "source": [
        "import altair as alt\n",
        "import pandas as pd\n",
        "source=pd.DataFrame({'project':['a','b','c','d','e','f','g'],'score':[55,24,10,28,35,10,45],'goal':[50,21,1,15,40,25,10]})\n",
        "a=alt.Chart(source).mark_bar().encode(x='project',y='score').properties(width=alt.Step(50))\n",
        "b=a.mark_tick(color='red',thickness=3,size=45).encode(y='goal')\n",
        "(a+b)"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "execute_result",
          "data": {
            "text/plain": [
              "alt.LayerChart(...)"
            ],
            "text/html": [
              "\n",
              "<div id=\"altair-viz-dc0c6bcb581249dab848697fb8b0dc6c\"></div>\n",
              "<script type=\"text/javascript\">\n",
              "  (function(spec, embedOpt){\n",
              "    let outputDiv = document.currentScript.previousElementSibling;\n",
              "    if (outputDiv.id !== \"altair-viz-dc0c6bcb581249dab848697fb8b0dc6c\") {\n",
              "      outputDiv = document.getElementById(\"altair-viz-dc0c6bcb581249dab848697fb8b0dc6c\");\n",
              "    }\n",
              "    const paths = {\n",
              "      \"vega\": \"https://cdn.jsdelivr.net/npm//vega@5?noext\",\n",
              "      \"vega-lib\": \"https://cdn.jsdelivr.net/npm//vega-lib?noext\",\n",
              "      \"vega-lite\": \"https://cdn.jsdelivr.net/npm//vega-lite@4.8.1?noext\",\n",
              "      \"vega-embed\": \"https://cdn.jsdelivr.net/npm//vega-embed@6?noext\",\n",
              "    };\n",
              "\n",
              "    function loadScript(lib) {\n",
              "      return new Promise(function(resolve, reject) {\n",
              "        var s = document.createElement('script');\n",
              "        s.src = paths[lib];\n",
              "        s.async = true;\n",
              "        s.onload = () => resolve(paths[lib]);\n",
              "        s.onerror = () => reject(`Error loading script: ${paths[lib]}`);\n",
              "        document.getElementsByTagName(\"head\")[0].appendChild(s);\n",
              "      });\n",
              "    }\n",
              "\n",
              "    function showError(err) {\n",
              "      outputDiv.innerHTML = `<div class=\"error\" style=\"color:red;\">${err}</div>`;\n",
              "      throw err;\n",
              "    }\n",
              "\n",
              "    function displayChart(vegaEmbed) {\n",
              "      vegaEmbed(outputDiv, spec, embedOpt)\n",
              "        .catch(err => showError(`Javascript Error: ${err.message}<br>This usually means there's a typo in your chart specification. See the javascript console for the full traceback.`));\n",
              "    }\n",
              "\n",
              "    if(typeof define === \"function\" && define.amd) {\n",
              "      requirejs.config({paths});\n",
              "      require([\"vega-embed\"], displayChart, err => showError(`Error loading script: ${err.message}`));\n",
              "    } else if (typeof vegaEmbed === \"function\") {\n",
              "      displayChart(vegaEmbed);\n",
              "    } else {\n",
              "      loadScript(\"vega\")\n",
              "        .then(() => loadScript(\"vega-lite\"))\n",
              "        .then(() => loadScript(\"vega-embed\"))\n",
              "        .catch(showError)\n",
              "        .then(() => displayChart(vegaEmbed));\n",
              "    }\n",
              "  })({\"config\": {\"view\": {\"continuousWidth\": 400, \"continuousHeight\": 300}}, \"layer\": [{\"mark\": \"bar\", \"encoding\": {\"x\": {\"type\": \"nominal\", \"field\": \"project\"}, \"y\": {\"type\": \"quantitative\", \"field\": \"score\"}}, \"width\": {\"step\": 50}}, {\"mark\": {\"type\": \"tick\", \"color\": \"red\", \"size\": 45, \"thickness\": 3}, \"encoding\": {\"x\": {\"type\": \"nominal\", \"field\": \"project\"}, \"y\": {\"type\": \"quantitative\", \"field\": \"goal\"}}, \"width\": {\"step\": 50}}], \"data\": {\"name\": \"data-cad82fd6b3c875a650d4c629514aef93\"}, \"$schema\": \"https://vega.github.io/schema/vega-lite/v4.8.1.json\", \"datasets\": {\"data-cad82fd6b3c875a650d4c629514aef93\": [{\"project\": \"a\", \"score\": 55, \"goal\": 50}, {\"project\": \"b\", \"score\": 24, \"goal\": 21}, {\"project\": \"c\", \"score\": 10, \"goal\": 1}, {\"project\": \"d\", \"score\": 28, \"goal\": 15}, {\"project\": \"e\", \"score\": 35, \"goal\": 40}, {\"project\": \"f\", \"score\": 10, \"goal\": 25}, {\"project\": \"g\", \"score\": 45, \"goal\": 10}]}}, {\"mode\": \"vega-lite\"});\n",
              "</script>"
            ]
          },
          "metadata": {},
          "execution_count": 358
        }
      ]
    },
